{
  "name": "tweetdeck_emoji",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "replace emoji",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://tweetdeck.twitter.com/"],
      "js": ["replace.js"]
    }
  ],
  "options_page": "options.html"
}
